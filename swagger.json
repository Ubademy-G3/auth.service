        }
    },
    "host": "localhost:3005",
    "basePath": "/auth",
    "tags": [
        {
            "name": "Auth",
            "description": ""
        }
    ],
    "schemes": [
        "http",
        "https"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/users": {
            "get": {
                "summary": "Get all registered users",
                "description": "Returns a list containing all users",
                "responses": {
                    "200": {
                        "description": "A list of a user",
                        "schema": {
                            "type": "array",
                            "items": {
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "token": {
                                        "type": "string"
                                    }
                                }

                            }
                        }
                    }
                }
            }
        },
        "/signup": {
            "post": {
                "summary": "Register a new user",
                "description": "Returns created user",
                "parameters": [
                    {
                        "name": "user",
                        "in": "body",
                        "description": "The user to register",
                        "schema": {
                            "required": [
                                "email",
                                "password"
                            ],
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "New user info",
                        "schema": {
                            "type": "object",
                            "items": {
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "password"
                                    },
                                    "token": {
                                        "type": "string"
                                    }
                                }

                            }
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "summary": "Log a user",
                "description": "Returns logged user",
                "parameters": [
                    {
                        "name": "user",
                        "in": "body",
                        "description": "The user to log",
                        "schema": {
                            "required": [
                                "email",
                                "password"
                            ],
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Logged user info",
                        "schema": {
                            "type": "object",
                            "items": {
                                "properties": {
                                    "id": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "password"
                                    },
                                    "token": {
                                        "type": "string"
                                    }
                                }

                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {

    }
}